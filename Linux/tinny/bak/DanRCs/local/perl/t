VAR HOUR_RISK : HOUR_RISK_DATA
GRP = risk
CALC =
VAR
   tmp_risk : HOUR_RISK_DATA
BEGIN
   Table_Lookup(CURRENT_HOUR,tmp_risk);
   return(tmp_risk);
ENDVAR;

VAR HOUR_RISK_RATE : FLOAT
GRP = risk
RCOD = 0
CALC =
FAL_RISK_RATE(adjust_ratio, HOUR_RISK.NUMFRDS, 
HOUR_RISK.TOTAL, def_risk_rate);

