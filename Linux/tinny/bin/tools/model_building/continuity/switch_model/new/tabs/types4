  TOTAL     : INT32;
  NUMFRDS   : INT32;
  NFMEANAMT, FMEANAMT, NFSTDAMT, FSTDAMT     : FLOAT;
  NFMEANDIST, FMEANDIST, NFSTDDIST, FSTDDIST : FLOAT;
END;

TYPE  CNTRY_RISK_DATA = RECORD
