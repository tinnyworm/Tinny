TYPE  NMON_RISK_DATA = RECORD
  NMON      : CHAR;
  FRm60p30  : FLOAT;
  FRm45p30  : FLOAT;
  FRm30p30  : FLOAT;
END;

TYPE  GEO_RISK_DATA = RECORD
  KEY       : STRING{4};
  TOTAL     : INT32;
  NUMFRDS   : INT32;
  NFMEANAMT, FMEANAMT, NFSTDAMT, FSTDAMT     : FLOAT;
  NFMEANDIST, FMEANDIST, NFSTDDIST, FSTDDIST : FLOAT;
END;

TYPE  HOW_RISK_DATA = RECORD
