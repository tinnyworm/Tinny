@#!/usr/local/bin/perl -w
#
# Get the factors of the inputs.
#
#


while (<>) {
    chomp;
    print "Factors of $_ are ";
    @@fact = &getFactors ($_);
    for ($i = 0; $i < scalar (@@fact) - 1; $i++) {
        print "$fact[$i], ";
    }
    print $fact[scalar (@@fact) - 1], "\n";
}

############################################################

sub getFactors () {
    use strict;
    my ($in) = @@_;
    my ($i, $j);
    my (@@retVal) = ();

    for ($i = 1; $i <= $in; $i++) {
        $j = $in / $i;
        if ($j == int($j)) {
            push @@retVal, $i;
        }
    }

    return (@@retVal);
}


