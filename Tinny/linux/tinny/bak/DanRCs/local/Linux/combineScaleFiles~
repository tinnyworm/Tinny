#!/usr/local/bin/bash
#
# run-time wrapper for class 'CombineScaleFiles'
MAIN=com.hnc.falcon.modeling.utils.scaleFiles.CombineScaleFiles
JAR_FILE=scaleFiles.jar
JAR_DIR=/work/price/falcon/dvl/java/jar

OS=`uname`
if [ $OS = 'Linux' ]
then
    export JAVA_HOME=/opt/IBMJava2-13
else
    export JAVA_HOME=/usr/java1.2
fi

export CLASSPATH=${JAR_DIR}/${JAR_FILE}
exec ${JAVA_HOME}/bin/java ${MAIN} "$@"
