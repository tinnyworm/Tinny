all :: dprofpp
	@$(NOOP)

dprofpp :: dprofpp.PL
	$(PERL) -I$(INST_ARCHLIB) -I$(INST_LIB) -I$(PERL_ARCHLIB) -I$(PERL_LIB) dprofpp.PL


